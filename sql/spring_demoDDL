-- 테이블 spring_demo.tb_user 구조 내보내기
DROP TABLE IF EXISTS `tb_user`;
CREATE TABLE IF NOT EXISTS `tb_user` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `accountId` bigint(20) NOT NULL COMMENT '계정ID',
  `phoneNumberId` bigint(20) NOT NULL COMMENT '폰번호ID',
  `activeFlag` varchar(1) COLLATE utf8mb4_bin NOT NULL COMMENT '활성화여부',
  `lastPracticeDatetime` datetime DEFAULT NULL COMMENT '최근실행일시',
  `telecomName` varchar(100) COLLATE utf8mb4_bin DEFAULT NULL COMMENT 'Network 연결된 이동통신사',
  `createDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '생성일시',
  `createHost` varchar(40) COLLATE utf8mb4_bin NOT NULL COMMENT '생성ID',
  `updateDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '수정일시',
  `updateHost` varchar(40) COLLATE utf8mb4_bin NOT NULL COMMENT '수정ID',
  PRIMARY KEY (`id`),
  UNIQUE KEY `ux1_user` (`accountId`,`phoneNumberId`),
  KEY `ix1_user` (`phoneNumberId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin COMMENT='사용자';

