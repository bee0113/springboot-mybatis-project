-- 테이블 spring_demo.bbs_file 구조 내보내기
DROP TABLE IF EXISTS `bbs_file`;
CREATE TABLE IF NOT EXISTS `bbs_file` (
  `fileId` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'idx',
  `boardId` bigint(20) NOT NULL COMMENT '게시판ID',
  `originFilename` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '실제파일이름',
  `saveFilename` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '서버저장파일이름',
  `fileSize` bigint(50) DEFAULT 0 COMMENT '파일사이즈',
  `createDatetime` datetime DEFAULT current_timestamp() COMMENT '파일 생성일시',
  `fileLocation` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '파일 경로',
  `delYn` varchar(1) COLLATE utf8mb4_bin DEFAULT 'N' COMMENT '파일삭제여부',
  PRIMARY KEY (`fileId`),
  KEY `FK_bbs_board_TO_bbs_file` (`boardId`),
  CONSTRAINT `FK_bbs_board_TO_bbs_file` FOREIGN KEY (`boardId`) REFERENCES `bbs_board` (`boardId`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=56 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin COMMENT='파일';

-- 테이블 데이터 spring_demo.bbs_file:~0 rows (대략적) 내보내기
/*!40000 ALTER TABLE `bbs_file` DISABLE KEYS */;
/*!40000 ALTER TABLE `bbs_file` ENABLE KEYS */;

-- 테이블 spring_demo.bbs_new_account 구조 내보내기
DROP TABLE IF EXISTS `bbs_new_account`;
CREATE TABLE IF NOT EXISTS `bbs_new_account` (
  `accountId` bigint(20) NOT NULL AUTO_INCREMENT COMMENT '계정ID',
  `accountUserId` varchar(80) NOT NULL COMMENT '사용자 아이디',
  `accountUserNm` varchar(45) NOT NULL COMMENT '사용자 이름',
  `accountEmail` varchar(80) NOT NULL COMMENT '사용자 이메일',
  `accountPassword` varchar(80) NOT NULL COMMENT '사용자 패스워드',
  `devCheck` varchar(5) NOT NULL COMMENT '개발자구분 - 예비(P), 경력(C)',
  `createDt` datetime NOT NULL DEFAULT current_timestamp() COMMENT '사용자 가입날짜',
  `updateDt` datetime NOT NULL DEFAULT current_timestamp() COMMENT '사용자 정보 수정 날짜',
  PRIMARY KEY (`accountId`),
  UNIQUE KEY `UQ_bbs_new_account_1` (`accountUserId`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8mb4 COMMENT='new 계정';

-- 테이블 데이터 spring_demo.bbs_new_account:~0 rows (대략적) 내보내기
/*!40000 ALTER TABLE `bbs_new_account` DISABLE KEYS */;
INSERT IGNORE INTO `bbs_new_account` (`accountId`, `accountUserId`, `accountUserNm`, `accountEmail`, `accountPassword`, `devCheck`, `createDt`, `updateDt`) VALUES
	(1, 'admin', 'admin', 'admin@gmail.com', '1234', 'P', '2021-05-27 16:39:25', '2021-05-27 16:39:25'),
	(2, 'test', 'test', 'test@mail.com', '1234', 'P', '2021-05-27 16:50:55', '2021-05-27 16:50:55'),
	(3, 'test2', '테스트', 'test2', '1234', 'C', '2021-05-27 17:01:00', '2021-05-27 17:01:00');
/*!40000 ALTER TABLE `bbs_new_account` ENABLE KEYS */;

-- 테이블 spring_demo.bbs_reply 구조 내보내기
DROP TABLE IF EXISTS `bbs_reply`;
CREATE TABLE IF NOT EXISTS `bbs_reply` (
  `replyId` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'idx',
  `accountId` bigint(20) NOT NULL COMMENT '계정ID',
  `boardId` bigint(20) NOT NULL COMMENT '게시판ID',
  `replyWriter` varchar(60) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '댓글작성자',
  `replyContent` varchar(2000) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '댓글내용',
  `replyDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '댓글생성일시',
  PRIMARY KEY (`replyId`),
  KEY `accountId` (`accountId`),
  KEY `boardId` (`boardId`),
  CONSTRAINT `accountId` FOREIGN KEY (`accountId`) REFERENCES `bbs_account` (`accountId`) ON DELETE CASCADE,
  CONSTRAINT `boardId` FOREIGN KEY (`boardId`) REFERENCES `bbs_board` (`boardId`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=33 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin COMMENT='게시판댓글';

