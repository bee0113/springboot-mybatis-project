-- 테이블 spring_demo.tb_sys_qna 구조 내보내기
DROP TABLE IF EXISTS `tb_sys_qna`;
CREATE TABLE IF NOT EXISTS `tb_sys_qna` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `accountEmail` varchar(80) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '계정이메일',
  `userPhoneNumber` varchar(20) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '사용자폰번호',
  `replyEmailAddress` varchar(60) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '이메일주소',
  `questionContent` varchar(1800) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '문의내용',
  `replyFlag` varchar(1) COLLATE utf8mb4_bin NOT NULL DEFAULT 'N' COMMENT '답변여부',
  `questionTypeCode` varchar(3) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '문의유형코드',
  `replyContent` varchar(4000) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '답변내용',
  `replyId` varchar(10) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '답변ID',
  `replyDatetime` datetime DEFAULT NULL COMMENT '답변일시',
  `replyEmailFlag` varchar(1) COLLATE utf8mb4_bin NOT NULL DEFAULT 'N' COMMENT '답변이메일여부',
  `replyEmailId` varchar(10) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '답변이메일ID',
  `replyEmailDatetime` datetime DEFAULT NULL COMMENT '답변이메일일시',
  `qnaTypeCode` varchar(1) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '질의응답유형코드',
  `createDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '생성일시',
  `createHost` varchar(40) COLLATE utf8mb4_bin NOT NULL COMMENT '생성ID',
  `updateDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '수정일시',
  `updateHost` varchar(40) COLLATE utf8mb4_bin NOT NULL COMMENT '수정ID',
  PRIMARY KEY (`id`),
  KEY `ix_sys_qna` (`accountEmail`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin COMMENT='질의응답';

-- 테이블 데이터 spring_demo.tb_sys_qna:~0 rows (대략적) 내보내기
/*!40000 ALTER TABLE `tb_sys_qna` DISABLE KEYS */;
