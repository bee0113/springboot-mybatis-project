-- 테이블 spring_demo.tb_user_account 구조 내보내기
DROP TABLE IF EXISTS `tb_user_account`;
CREATE TABLE IF NOT EXISTS `tb_user_account` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `accountEmail` varchar(80) COLLATE utf8mb4_bin NOT NULL COMMENT '계정이메일',
  `accountPassword` varchar(80) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '계정암호[IOS]',
  `birthDay` varchar(8) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '생년월일[AN]',
  `sexCode` varchar(1) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '성별코드[AN]',
  `openScopeCode` varchar(1) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '공개범위코드(A=모두공개, F=친구만공개, B=모두비공개)',
  `countryCode` varchar(10) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '국가코드',
  `joinDivisionCode` varchar(2) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '가입구분코드[IOS] (F:FACEBOOK, G:GOOGLE, E:EMAIL)',
  `userName` varchar(60) COLLATE utf8mb4_bin DEFAULT NULL COMMENT '사용자명',
  `firstPracticeDatetime` datetime DEFAULT NULL COMMENT '최초실행일시',
  `lastPracticeDatetime` datetime DEFAULT NULL COMMENT '최근실행일시',
  `createDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '생성일시',
  `createHost` varchar(40) COLLATE utf8mb4_bin NOT NULL COMMENT '생성ID',
  `updateDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '수정일시',
  `updateHost` varchar(40) COLLATE utf8mb4_bin NOT NULL COMMENT '수정ID',
  PRIMARY KEY (`id`),
  UNIQUE KEY `ux_user_account` (`accountEmail`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin COMMENT='계정';

