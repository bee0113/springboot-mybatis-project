-- 테이블 spring_demo.bbs_survey_item_cd 구조 내보내기
DROP TABLE IF EXISTS `bbs_survey_item_cd`;
CREATE TABLE IF NOT EXISTS `bbs_survey_item_cd` (
  `itemId` bigint(20) NOT NULL AUTO_INCREMENT COMMENT '항목번호',
  `groupCd` varchar(20) NOT NULL COMMENT '항목 그룹코드',
  `itemCd` varchar(45) NOT NULL COMMENT '항목 코드',
  `itemNm` varchar(45) NOT NULL COMMENT '노출될 항목명',
  `itemComm` varchar(100) DEFAULT NULL COMMENT '항목 설명',
  PRIMARY KEY (`itemId`,`itemCd`)
) ENGINE=InnoDB AUTO_INCREMENT=72 DEFAULT CHARSET=utf8mb4 COMMENT='설문 항목코드';

