-- 테이블 spring_demo.bbs_attach 구조 내보내기
DROP TABLE IF EXISTS `bbs_attach`;
CREATE TABLE IF NOT EXISTS `bbs_attach` (
  `uuid` varchar(100) COLLATE utf8mb4_bin NOT NULL,
  `uploadPath` varchar(200) COLLATE utf8mb4_bin NOT NULL,
  `fileName` varchar(100) COLLATE utf8mb4_bin NOT NULL,
  `filetype` char(1) COLLATE utf8mb4_bin DEFAULT 'I',
  `boardId` bigint(20) DEFAULT NULL COMMENT '게시물ID',
  PRIMARY KEY (`uuid`),
  KEY `FK_bbs_board_TO_bbs_attach` (`boardId`),
  CONSTRAINT `FK_bbs_board_TO_bbs_attach` FOREIGN KEY (`boardId`) REFERENCES `bbs_board` (`boardId`) ON DELETE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin COMMENT='파일2';

-- 테이블 데이터 spring_demo.bbs_attach:~7 rows (대략적) 내보내기
/*!40000 ALTER TABLE `bbs_attach` DISABLE KEYS */;
