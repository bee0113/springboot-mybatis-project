-- 테이블 spring_demo.tbl_attach 구조 내보내기
DROP TABLE IF EXISTS `tbl_attach`;
CREATE TABLE IF NOT EXISTS `tbl_attach` (
  `uuid` varchar(100) COLLATE utf8mb4_bin NOT NULL COMMENT '파일UUID',
  `uploadPath` varchar(200) COLLATE utf8mb4_bin NOT NULL COMMENT '파일경로',
  `fileName` varchar(100) COLLATE utf8mb4_bin NOT NULL COMMENT '파일명',
  `filetype` char(1) COLLATE utf8mb4_bin DEFAULT 'I' COMMENT '파일타입(이미지,일반)',
  `bno` bigint(50) DEFAULT NULL COMMENT '게시물idx',
  PRIMARY KEY (`uuid`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin;

-- 테이블 데이터 spring_demo.tbl_attach:~0 rows (대략적) 내보내기
