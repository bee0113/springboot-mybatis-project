-- 테이블 spring_demo.bbs_survey 구조 내보내기
DROP TABLE IF EXISTS `bbs_survey`;
CREATE TABLE IF NOT EXISTS `bbs_survey` (
  `surveyId` bigint(20) NOT NULL AUTO_INCREMENT COMMENT '설문ID',
  `MajorItem` varchar(10) DEFAULT NULL COMMENT '전공/비전공 항목',
  `nonMajorStudyItem` varchar(10) DEFAULT NULL COMMENT '비전공자 학습방법',
  `ageItem` varchar(10) DEFAULT NULL COMMENT '연령대',
  `careerItem` varchar(10) DEFAULT NULL COMMENT '경력',
  `IncomeItem` varchar(10) DEFAULT NULL COMMENT '연소득범위',
  `jobItem` varchar(10) DEFAULT NULL COMMENT '현재 직무',
  `useOSItem` varchar(10) DEFAULT NULL COMMENT '주로 사용하는 OS',
  `useDBItem` varchar(10) DEFAULT NULL COMMENT '주로 사용하는 DB',
  `useEditorItem` varchar(10) DEFAULT NULL COMMENT '주로 사용하는 Editor',
  `confidentLangItem` varchar(10) DEFAULT NULL COMMENT '자신있는 언어',
  `learnLangItem` varchar(10) DEFAULT NULL COMMENT '배우고 싶은 언어',
  `confidentFrmItem` varchar(10) DEFAULT NULL COMMENT '자신있는 프레임워크 / 라이브러리',
  `learnFrmItem` varchar(10) DEFAULT NULL COMMENT '배우고 싶은 프레임워크 / 라이브러리',
  `accountId` bigint(20) NOT NULL COMMENT '계정ID',
  `regDatetime` datetime NOT NULL DEFAULT current_timestamp() COMMENT '설문 등록날짜',
  PRIMARY KEY (`surveyId`),
  UNIQUE KEY `UQ_bbs_survey_1` (`accountId`),
  CONSTRAINT `FK_bbs_survey_bbs_new_account_accountId` FOREIGN KEY (`accountId`) REFERENCES `bbs_new_account` (`accountId`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='설문';

